{
  "manifest_version": 3,
  "name": "Docs Vim Motions",
  "description": "Vim Moions within Google Docs",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {},
  "permissions": ["scripting", "activeTab"]
}

