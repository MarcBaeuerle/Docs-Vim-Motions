{
  "name": "Docs-Vim-Motions",
  "description": "Vim Motions with Google Docs",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action" : {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://docs.google.com/document/*"],
      "js": ["./content.js"]
    }
  ]
}
